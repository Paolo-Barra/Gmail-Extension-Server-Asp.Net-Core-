{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "plugins": [
      {
        "name": "@angular/language-service"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "*": [
        "types/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@abp/*": [
        "../node_modules/abp-ng2-module/dist/src/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@account/*": [
        "./account/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@rshared/*": [
        "./app/refind/shared/*"
      ],
      "@rshared1/*": [
        "./shared/*"
      ],
      "@node_modules/*": [
        "../node_modules/*"
      ],
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "@metronic/*": [
        "./assets/metronic/*"
      ],
      "@refind/*": [
        "./app/refind/*"
      ],
      "@refindRoot/*": [
        "./refindRoot/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}